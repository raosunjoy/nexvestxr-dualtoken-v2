  "compliance": {
    "regulators": {
      "RERA": {
        "name": "Real Estate Regulatory Agency",
        "enabled": true,
        "apiEndpoint": "https://api.rera.ae",
        "required": true
      },
      "DLD": {
        "name": "Dubai Land Department",
        "enabled": true,
        "apiEndpoint": "https://api.dubailand.gov.ae",
        "required": true
      },
      "ADGM": {
        "name": "Abu Dhabi Global Market",
        "enabled": true,
        "apiEndpoint": "https://api.adgm.com",
        "required": false
      },
      "DFSA": {
        "name": "Dubai Financial Services Authority",
        "enabled": true,
        "apiEndpoint": "https://api.dfsa.ae",
        "required": false
      }
    },
    "kyc": {
      "levels": {
        "standard": ["emirates_id", "passport"],
        "enhanced": ["emirates_id", "passport", "salary_certificate"],
        "comprehensive": ["emirates_id", "passport", "salary_certificate", "bank_statement", "trade_license"]
      },
      "international": {
        "enabled": true,
        "documents": ["passport", "proof_of_address", "bank_statement"],
        "fatfCompliance": true
      }
    }
